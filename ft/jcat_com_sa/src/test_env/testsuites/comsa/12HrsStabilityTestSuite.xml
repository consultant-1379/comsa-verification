	    	<hw>['system','switches','network']</hw>
	    	<os>['lotc']</os>
	    	<mw>['coreMw']</mw>
	    	<appl>[]</appl>
	    	<interfaces>['ctrl1','ctrl2']</interfaces>
	    	<events>['dumps','address','log','actCtrl']</events>
