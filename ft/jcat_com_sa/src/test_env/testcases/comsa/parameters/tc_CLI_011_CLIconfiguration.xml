<?xml version="1.0" encoding="utf-8"?>
<testcase>
    <info>
        <id>CLI change configuration - deleting an object and aborting the change</id>
        <tag>TC-CLI-011</tag>	
        <version>P1A01</version>
		<testcase>cliConfigurationTestCase</testcase>
		<class>CLIconfiguration</class>
		<method>CLIconfiguration</method>
		<description>CLI change configuration - deleting an object and aborting the change</description>
	</info>	
    <config>
    	<cli_input_1>"show ManagedElement=1, SystemFunctions=1, SysM=1, Snmp=1, SnmpTargetV1=1" "exit"</cli_input_1>
        <cli_expected_output_1>['ERROR: Specific element not found']</cli_expected_output_1>
        <cli_nonexpected_output_1>['closed connection', 'Connection to COM failed']</cli_nonexpected_output_1>
        
        <cli_input_2>"configure" "ManagedElement=1,SystemFunctions=1,SysM=1,Snmp=1" "SnmpTargetV1=1" "address=1" "community=2" "commit" "exit"</cli_input_2>
        <cli_expected_output_2>['ManagedElement=1,SystemFunctions=1,SysM=1,Snmp=1', 'SnmpTargetV1=1', 'commit']</cli_expected_output_2>
        <cli_nonexpected_output_2>['error', 'closed connection', 'Connection to COM failed']</cli_nonexpected_output_2>
        
        <cli_input_3>"show ManagedElement=1,SystemFunctions=1,SysM=1,Snmp=1,SnmpTargetV1=1" "exit"</cli_input_3>
        <cli_expected_output_3>['show ManagedElement=1,SystemFunctions=1,SysM=1,Snmp=1,SnmpTargetV1=1']</cli_expected_output_3>
        <cli_nonexpected_output_3>['error', 'closed connection', 'Connection to COM failed']</cli_nonexpected_output_3>
        
        <cli_input_4>"configure" "ManagedElement=1,SystemFunctions=1,SysM=1,Snmp=1" "no SnmpTargetV1=1" "abort" "exit"</cli_input_4>
        <cli_expected_output_4>['ManagedElement=1,SystemFunctions=1,SysM=1,Snmp=1', 'no SnmpTargetV1=1', 'abort']</cli_expected_output_4>
        <cli_nonexpected_output_4>['error', 'closed connection', 'Connection to COM failed']</cli_nonexpected_output_4>
        
        <cli_input_5>"show ManagedElement=1, SystemFunctions=1, SysM=1, Snmp=1, SnmpTargetV1=1" "exit"</cli_input_5>
        <cli_expected_output_5>['show ManagedElement=1, SystemFunctions=1, SysM=1, Snmp=1, SnmpTargetV1=1']</cli_expected_output_5>
        <cli_nonexpected_output_5>['error', 'closed connection', 'Connection to COM failed']</cli_nonexpected_output_5>
        
        <cli_input_6>"configure" "ManagedElement=1,SystemFunctions=1,SysM=1,Snmp=1" "no SnmpTargetV1=1" "commit" "exit"</cli_input_6>
        <cli_expected_output_6>['ManagedElement=1,SystemFunctions=1,SysM=1,Snmp=1', 'no SnmpTargetV1=1', 'commit']</cli_expected_output_6>
        <cli_nonexpected_output_6>['error', 'closed connection', 'Connection to COM failed']</cli_nonexpected_output_6>
        
        <cli_input_7>"show ManagedElement=1, SystemFunctions=1, SysM=1, Snmp=1, SnmpTargetV1=1" "exit"</cli_input_7>
        <cli_expected_output_7>['ERROR: Specific element not found']</cli_expected_output_7>
        <cli_nonexpected_output_7>['closed connection', 'Connection to COM failed']</cli_nonexpected_output_7>
	</config>  
</testcase>