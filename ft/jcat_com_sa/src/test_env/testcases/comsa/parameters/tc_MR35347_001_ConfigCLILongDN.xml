<?xml version="1.0" encoding="utf-8"?>
<testcase>
	<info>
		<id>Test for support Long DN using CLI</id>
		<tag>TC-MR35347-001</tag>
		<version>P1A01</version>
		<testcase>functionTestMr35347</testcase>
		<class>functionTestMr35347</class>
		<method>functionTestMr35347</method>
		<description>Changing an attribute value using CLI config</description>
	</info>
	<config>

		<reqComSaVersion>R6A07</reqComSaVersion>
		<reqComVersion>R2A12</reqComVersion>
		<reqCmwVersion>R10A01</reqCmwVersion>
		<reqComSaRelease>3</reqComSaRelease>
		<reqComRelease>2</reqComRelease>
		<reqCmwRelease>1</reqCmwRelease>
		<cli_input_1>"configure" "ManagedElement=1,SystemFunctions=1,SysM=1" "" "serverAddress=1.1.1.1" "administrativeState=UNLOCKED" "commit" "exit" </cli_input_1>
		<cli_expected_output_1>['']</cli_expected_output_1>
		<cli_nonexpected_output_1>['closed connection', 'Connection to COM failed', 'ERROR: Transaction validation failed with error code: ComFailure']</cli_nonexpected_output_1>

		<cli_input_2>"configure" "ManagedElement=1,SystemFunctions=1,SysM=1" "" "serverAddress=1.1.1.1" "administrativeState=UNLOCKED" "commit" "exit" </cli_input_2>
		<cli_expected_output_2>['ERROR: Transaction not committed due to validation errors|Transaction validation failed with error code: ComFailure']</cli_expected_output_2>
		<cli_nonexpected_output_2>['closed connection', 'Connection to COM failed']</cli_nonexpected_output_2>

		<cli_input_3>"configure" "ManagedElement=1,SystemFunctions=1,SysM=1" "no " "commit" "exit" </cli_input_3>
		<cli_expected_output_3>['no ']</cli_expected_output_3>
		<cli_nonexpected_output_3>['closed connection', 'Connection to COM failed']</cli_nonexpected_output_3>

	</config>

</testcase>
