package se.ericsson.jcat.omp.util;

class LibraryConfig {

    static int PYTHON = 1;

    static int JAVA = 2;

    private final int language;

    public int getLanguage() {
        return language;
    }

    public String getClassOrModule() {
        return classOrModule;
    }

    private final String classOrModule;

    LibraryConfig(final int language, final String classOrModule) {
        this.language = language;
        this.classOrModule = classOrModule;

    }

}