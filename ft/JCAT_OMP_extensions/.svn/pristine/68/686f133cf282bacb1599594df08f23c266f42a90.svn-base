package se.ericsson.jcat.omp.fw;

/**
 * Thrown by an OmpLibrary implementation when an API method error occurs.
 */
public class OmpLibraryException extends Exception {

    /**
     * TODO javadoc for <code>serialVersionUID</code>
     */
    private static final long serialVersionUID = 8546766677966938195L;

    /*
     * Exception should not be used without message
     */
    @SuppressWarnings("unused")
    private OmpLibraryException() {

    }

    /**
     * 
     * Creates a new instance of <code>OmpLibraryException</code> with a specified detail message.
     * 
     * @param message
     *            the detail message
     */
    public OmpLibraryException(String message) {
        super(message);
    }

    public OmpLibraryException(String message, Exception e) {
        super(message, e);
    }
}
